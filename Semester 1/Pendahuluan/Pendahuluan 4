package MODUL4;

/**
 *
 * @author asus
 */
public class Pendahuluan_modul_4 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
      for (int i=1;i<10;i++){
         System.out.println(i);
      }
      
      int i=4;
      while (i<10){
          System.out.println(i);
          i++;
      }
      
      
    }
    
}
